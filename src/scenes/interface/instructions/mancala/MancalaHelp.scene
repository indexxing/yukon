                            "label": "page4_text2",
                            "y": -88,
                            "originX": 0.5,
                            "originY": 0.5,
                            "text": "If the last stone lands in the players own mancala,\nthat player goes again.",
                            "fixedWidth": 750,
                            "align": "center",
                            "fontFamily": "Arial",
                            "fontSize": "32px",
                            "color": "#000000"
